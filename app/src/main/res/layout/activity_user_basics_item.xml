<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_user_basics_item"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBEBEB"
    tools:context="com.zzb.sl.UserBasicsInfo.UserBasicsItemActivity">
    <RelativeLayout
        android:id="@+id/timeContl_rl_Table"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/title"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/user_basics_item_img"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/right"
            android:layout_centerVertical="true"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/BasicsInfo"
                android:textColor="@color/bg_white"
                android:textSize="20dp" />
        </LinearLayout>
    </RelativeLayout>
    <com.maiml.library.BaseItemLayout
        android:id="@+id/user_basics_item_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_below="@id/timeContl_rl_Table"
        app:text_size  = "18"
        app:icon_margin_left = "10"
        app:icon_text_margin = "10"
        app:item_height = "50"
        app:right_text_margin = "10"
        >
    </com.maiml.library.BaseItemLayout>
</RelativeLayout>
