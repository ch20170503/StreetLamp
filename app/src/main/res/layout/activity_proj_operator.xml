<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_projoper_operator"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zzb.sl.UserBasicsInfo.PorjActivity.ProjOperatorActivity">
    <RelativeLayout
        android:id="@+id/projoper__Table"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/title"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/projoper__img2"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/right" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ProjectInformation"
                android:textColor="@color/bg_white"
                android:textSize="20dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:gravity="right">
        </LinearLayout>
    </RelativeLayout>
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/projoper_srfl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/projoper__Table">
        <com.zzb.sl.UserBasicsInfo.PorjActivity.projoperatorCustomView.ItemRemoveRecyclerView
            android:id="@+id/projoper_re_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EBEBEB">
        </com.zzb.sl.UserBasicsInfo.PorjActivity.projoperatorCustomView.ItemRemoveRecyclerView>
    </android.support.v4.widget.SwipeRefreshLayout>
</RelativeLayout>
